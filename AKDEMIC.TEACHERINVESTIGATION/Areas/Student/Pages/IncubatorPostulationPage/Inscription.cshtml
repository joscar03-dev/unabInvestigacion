                                <div class="m-form__group-sub col-lg-6">
                                    <label class="required-form-label">Objetivos Generales</label>
                                    <textarea name="GeneralGoals" class="form-control m-input" rows="4" required></textarea>
                                </div>
                                <div class="m-form__group-sub col-lg-6">
                                    <label class="required-form-label">Departamento</label>
                                    <select id="DepartmentId" name="DepartmentId" class="form-control" required>
                                        <option value="0" selected disabled>Seleccione un Departamento</option>
                                    </select>
                                </div>
                            </div>
