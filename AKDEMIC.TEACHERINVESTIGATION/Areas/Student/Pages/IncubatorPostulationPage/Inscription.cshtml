<!--Add Modal Objetivo Específico-->
<div class="modal fade" id="specificGoalAddModal" tabindex="-1000" role="dialog" aria-labelledby="detailLabel" aria-hidden="true">
    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">
                    Agregar Objetivo Específico/Componente
                </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form id="specific-goal-add-form" class="m-form m-form--state" asp-area="Student" asp-page-handler="SpecificGoalAdd" method="post">
                <input name="IncubatorPostulationId" value="@Model.IncubatorPostulationId" hidden />
                <div class="modal-body">
                    <div class="m-form__group">
                        <label class="required-form-label">Descripción:</label>
                        <input name="Description" type="text" class="form-control m-input" autocomplete="off" required />
                    </div>
                    <div class="m-form__group">
                        <label class="required-form-label">Orden:</label>
                        <input name="OrderNumber" type="text" class="form-control m-input" autocomplete="off" required />
                    </div>
                </div>
                <div class="modal-footer">
                    <div style="text-align:right;">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">
                            Cancelar
                        </button>
                        <button id="btnSpecificGoalAdd" type="submit" class="btn btn-primary ladda-button" data-style="slide-down">
